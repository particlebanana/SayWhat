!!!
!= cc_html(:lang => 'en') do
  %head
    %meta{"http-equiv"=>"Content-Type", :content=>"text/html; charset=utf-8"}
    %meta{"http-equiv" => "X-UA-Compatible", :content => "IE=edge,chrome=1"}
    
    %title
      = "Say What! Manager"
 
    = stylesheet_link_tag "admin.css"
    = javascript_include_tag "admin.js"

    // Typekit
    %script{ type: "text/javascript", src: "http://use.typekit.com/#{CONFIG['typekit_code']}.js" }

    :javascript
      try{Typekit.load();}catch(e){}
    
    = csrf_meta_tag

  %body
    %div#wrap    
      = render :partial => "layouts/partials/header"

      %div.container.cf
        = flash_messages(flash).html_safe
      
        = render :partial => "layouts/partials/admin_nav"

        = yield